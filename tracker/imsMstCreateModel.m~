function q = imsMstCreateModel(imObject, h)

	global BINS TARGET_SIZE;
	
	if (TARGET_SIZE == [])
		TARGET_SIZE = size(imObject);
	end
	
	h = max(size(imObject, 1), size(imObject, 2)) / max(TARGET_SIZE(1), TARGET_SIZE(2));
	
	imKernel = imsMstKernel(pi * h^2);
	
	q = imsHistogram(imObject, imKernel, BINS)
	

end